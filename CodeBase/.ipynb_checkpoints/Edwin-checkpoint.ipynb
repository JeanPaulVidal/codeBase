   "source": [
    "# Frecuencia Relativa\n",
    "def frecRel(frecAbs):\n",
    "    frecRel = []\n",
    "    frecAbsT = sum(frecAbs)\n",
    "    for element in frecAbs:\n",
    "        fr = 100 / frecAbsT * element\n",
    "        frecRel.append(fr)\n",
    "    return frecRel"
   ]
  },
